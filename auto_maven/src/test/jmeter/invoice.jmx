            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.ArrayList;
import java.util.List;
import java.util.Random;
import java.util.Map.Entry;
import java.util.Set;

import com.alibaba.fastjson.JSON;
import com.alibaba.fastjson.JSONObject;

String resp_data = prev.getResponseDataAsString(); //获取请求返回值，此处只能获取到String类型；
log.info(resp_data);
JSONObject jsonObject = JSONObject.parseObject(resp_data); //将获取到的resp_data转换为JSONObject类型
JSONObject retObj = jsonObject.getJSONObject(&quot;retObj&quot;); //使用get方法，获取retObj内json串
retObj.remove(&quot;id&quot;); //使用remove方法，删除请求中无需的属性
retObj.remove(&quot;belong&quot;);
retObj.remove(&quot;userId&quot;);
retObj.remove(&quot;createTime&quot;);
retObj.remove(&quot;chineseInvoiceAddress&quot;);
retObj.remove(&quot;chineseMailAddress&quot;);
retObj.remove(&quot;invoiceName&quot;);
retObj.remove(&quot;linker&quot;);
List list = new ArrayList(); //新建一个发票类型列表list
list.add(&quot;2&quot;); //添加列表字段，发票类型为专用发票2和电子发票3
list.add(&quot;3&quot;);
Random random = new Random(); 
int i = random.nextInt(list.size());
if (&quot;false&quot;.equals(&quot;${userType}&quot;)) {
	retObj.put(&quot;invoiceType&quot;, &quot;3&quot;); //判断用户不是一般纳税人，则申请电子发票
}else{
	retObj.put(&quot;invoiceType&quot;,list.get(i)); //判断用户是一般纳税人，则随机申请专用发票或电子发票
}
//Set&lt;Entry&lt;String, Object&gt;&gt; entrySet = retObj.entrySet();
Set entrySet = retObj.entrySet();
for (Entry entry : entrySet) { //如果用户未设置发票信息，则查询到的rebObj属性值为空，需要为其赋值
	if (entry.getValue() == null || &quot;&quot;.equals(entry.getValue())) {  
		if (&quot;taxpayerId&quot;.equals(entry.getKey())) { //判断key值
			entry.setValue(&quot;123456789ASDFGH&quot;); //为该key值赋值
		}if (&quot;linkTel&quot;.equals(entry.getKey())) {
			entry.setValue(&quot;13252012012&quot;);
		}if (&quot;bankName&quot;.equals(entry.getKey())) {
